      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.15.7"
  },
  "dependencies": {
    "@fluentui/merge-styles": "^8.2.0",
    "@fluentui/react": "^8.37.0",
    "react": "^18.0.0-alpha-5ca4b0433-20211020",
    "react-dom": "^18.0.0-alpha-5ca4b0433-20211020",
